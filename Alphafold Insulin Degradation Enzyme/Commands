from chimerax.core.commands import run
run(session, 'show c')
run(session, 'show a')
run(session, 'hide s')
run(session, 'style ball')
run(session, 'size ballscale 0.175')
run(session, 'color sequential palette Reds-5')
run(session, 'transparency 70 atoms')
run(session, 'lighting flat')
run(session, 'car style  protein modeh default arrows f xsect oval width 1 thick 1')
run(session, 'sequence chain all')
run(session, 'view orient')
run(session, 'ui tool show "Side View"')
run(session, 'graphics silhouettes true')
run(session, 'set bgColor #ffffff00')
